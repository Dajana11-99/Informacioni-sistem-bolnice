<UserControl x:Class="ZdravoKorporacija.PacijentPrikaz.Kalendar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:calendar="clr-namespace:MyCalendar.Calendar;assembly=MyCalendar.Calendar"
        mc:Ignorable="d"
         Height="300" Width="580">
    <Grid>
        <Label FontSize="25" HorizontalAlignment="Center" Margin="0 -15 0 0">Period uzimanja terapije</Label>
       
        <calendar:Calendar Margin="0 0 0 0" x:Name="Calendar" Width="580" Height="350"
                          />
    </Grid>
</UserControl>
