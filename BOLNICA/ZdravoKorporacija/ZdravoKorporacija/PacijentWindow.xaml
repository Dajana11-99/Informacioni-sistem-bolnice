        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoKorporacija"
        mc:Ignorable="d"
        Title="PacijentWindow" Height="550" Width="800">
    <Grid>
        <StackPanel >
            <DataGrid Height="300" Name="dgSimple" Margin="77,60,77,50"/>
            <StackPanel Orientation="Horizontal">
                <Button Height="50" Width="150" Margin="77,0,550,0" Click="Button_Click">Zakazi pregled</Button>
                <Button Height="50" Width="150" Margin="-500,0,550,0" Click="Button_Click_1" Content="Izmeni pregled"/>
                <Button Height="50" Width="150" Margin="-500,0,520.6,0" Click="Button_Click_2" Content="Otkazi pregled"/>
            </StackPanel>
          

        </StackPanel>


    </Grid>
</Window>
